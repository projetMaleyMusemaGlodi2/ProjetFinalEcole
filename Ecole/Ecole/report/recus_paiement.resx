  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>select codepay as Num_Recus,codeincription as Code_Inscription,nom as Nom,postnom as Post_Nom,montantpay as Montant,datepay as _Date,paiement.codefrais as Code_Frais,frais as Type_Frais,montantprev as Montant_Prevu,paiement.codeutil as Code_Utilisateur,utilisateur as Utilisateur,libelle as Note from paiement
inner join frais on frais.codefrais=paiement.codefrais
inner join inscription on inscription.codeinscription=paiement.codeincription
inner join Eleve on Eleve.codeeleve=inscription.codeeleve
inner join utilisateur on utilisateur.codeutil=paiement.codeutil
inner join prevision on prevision.codefrais=frais.codefrais</value>
